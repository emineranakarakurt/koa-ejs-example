        <div class="image-import__view">
            <div class="carte">
                <div class="divImage">
                    
                </div>
            </div>
            <div class="toolbar d-flex space-between align-center">
                <span>
                    <i class="fa-solid fa-magnifying-glass-plus zoom" title="Zoomer"></i>
                    <i class="fa-solid fa-magnifying-glass-minus dezoom" title="Dézoomer"></i>
                </span>
                <span>
                    <img class="brightnessPlus" src="./images/lum+.svg" alt="" title="Augmenter la luminosité"/>
                    <img class="brightnessMoins" src="./images/lum-.svg" alt="" title="Diminuer la luminosité"/>
                </span>
                <span>
                    <img class="contrastPlus" src="./images/contrast+.svg" alt="" title="Augmenter le contraste">
