<div class="container-interface ">
    <div class="fil-arianne align-self-start">
        <span><a href="/projectsolo">Importation image</a></span>
        <span> > </span>
        <span><a href="/generatedata">Generate Data</a></span>
        <span> > </span>
        <span><a href="/datapanel">Data panel</a></span>
        <span> > </span>
        <span><a href="/classification">Classifier</a></span>
        <span> > </span>
        <span><a href="#">Résultats</a></span>
    </div>
    <div class="results">
        <div class="onglet d-flex space-around align-center"><a href="/classification"><i class="fa-solid fa-circle-arrow-left"></i></a><span>Data panel</span><span class="btn-close">X</span></div>
        <div class="results-content results-col d-flex space-around">
                <div class="results-col__left d-flex column space-between">
                    <div class="d-flex align-end">
                        <div class="toolbar d-flex column">
                            <i class="fa-solid fa-magnifying-glass-plus"></i>
                            <i class="fa-solid fa-magnifying-glass-minus"></i>
                        </div>
                        <div class="stats-results">
                            
                            <h2>Projection panel</h2>
                            <div class="d-flex ">
                                <div class="stats-results__section">
                                    <div>
                                        <label for="method">Méthode de visualisation</label>
                                        <select name="method" id="method">
                                            <option value="isoMDS">isoMDS</option>
                                        </select>
                                    </div>
                                    <div>
                                        <label for="dimension">Dimension</label>
                                        <select name="dimension" id="dimension">
                                            <option value="2d">2D</option>
                                        </select>
                                    </div>
                                    <div>
                                        <label for="visualiser">Visualiser</label>
                                        <select name="visualiser" id="visualiser">
                                            <option value="visualiser">Random</option>
                                        </select>
                                    </div>
                                    <div>
                                        <label for="points-similaires">Plus de points similaires au cluster</label>
                                        <input type="range" name="points-similaires" id="points-similaires" value="10">
                                        <output name="points-similaires-output" id="points-similaires-output">10</output>
                                        <button class="btn-sm btn-primary">Refresh</button>
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-start space-between">
                        <div class="view-mode">
                            <h3>View mode</h3>
                            <div>
                                <input type="radio" name="viewmode" id="normal">
                                <label for="normal">Normal Mode</label>
                            </div>
                            <div>
                                <input type="radio" name="viewmode" id="multi">
                                <label for="multi">Multi Mode</label>
                            </div>
                        </div>
                        <div class="button-menu d-flex">
                            <a href="/results-projection-panel" class="active">Projection Panel</a>
                            <a href="/results-centroids">Centroids</a>
                            <a href="/results-cardinality">Cardinality</a>
                            <a href="/results-scatter-plot">Scatter plot</a>
                            <a href="/results-evaluation" >Evaluation</a>
                            <a href="/results-similarity">Similarity HeatMap</a>
                            <a href="/results-weights">Weights</a>
                            <a href="/results-image">Image result</a>
                            <a href="/results-historique">Historique</a>
                        </div>
                    </div>
                </div>
                <div class="results-col__right">
                    <%- include('./partials/results-right-side'); %>
                </div>
            </div>
        
    </div>
    <div class="infobar d-flex space-around align-center">
        <span>Sauvegarde auto</span>
        <span class="projet-name">Projet : XXX</span>
        <span>Date début projet : XXX</span>
    </div>
</div>