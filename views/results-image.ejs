        <div class="results-content results-col d-flex">
            <div class="results-col__left d-flex column space-between">
                <div class="d-flex align-end">
                    <div class="toolbar d-flex column">
                        <!--<i class="fa-solid fa-magnifying-glass-plus"></i>
                        <i class="fa-solid fa-magnifying-glass-minus"></i>-->
